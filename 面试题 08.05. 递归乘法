class Solution {
    public int multiply(int A, int B) {
        if (A == 0 || B == 0) {
            return 0;
        }
        return dfs(A, B, A);
    }
    public int dfs(int sum, int num, int n) {
        if (num > 1) {
            return dfs(sum + n, num - 1, n);
        }
        return sum;
    }
}
